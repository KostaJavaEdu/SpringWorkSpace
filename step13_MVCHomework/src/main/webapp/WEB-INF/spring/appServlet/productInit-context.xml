<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean class="kosta.mvc.model.dto.ProductDTO" id="product1">
	<property name="code" value="10"/>
	<property name="name" value="아이폰"/>
	<property name="price" value="3000000"/>
	<property name="detail" value="카메라3개"/>
</bean>

<bean class="kosta.mvc.model.dto.ProductDTO" id="product2">
	<property name="code" value="20"/>
	<property name="name" value="갤럭시"/>
	<property name="price" value="4000000"/>
	<property name="detail" value="노트펜"/>
</bean>

<bean class="kosta.mvc.model.dto.ProductDTO" id="product3">
	<property name="code" value="30"/>
	<property name="name" value="샤오미"/>
	<property name="price" value="5000000"/>
	<property name="detail" value="가성비 굿"/>
</bean>
</beans>
