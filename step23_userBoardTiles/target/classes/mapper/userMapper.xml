<mapper namespace="userMapper">

	<!-- 로그인 -->
	<select id="loginCheck" parameterType="member" resultType="member">
	   select user_id,pwd,name from member where user_id=#{userId} and pwd=#{pwd}
	</select>
	

	
</mapper>